{
  "name": "Link Here",
  "description": "A chrome extension which constructs a text node link and copies it.",
  "version": "0.1",
  "permissions": [
  	"contextMenus", "activeTab"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "icons" : {
      "16" : "16x16.png",
      "32" : "32x32.png",
      "64" : "64x64.png"
  },
  "manifest_version": 2
}
